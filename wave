onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /Full_adsub/a
add wave -noupdate /Full_adsub/b
add wave -noupdate /Full_adsub/cin
add wave -noupdate /Full_adsub/x
add wave -noupdate /Full_adsub/y
